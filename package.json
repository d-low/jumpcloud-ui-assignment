{
  "name": "jumpcloud-ui-assignment",
  "version": "1.0.0",
  "description": "Front end code for JumpCloud's UI Assmentment Project",
  "repository": {
    "type": "git",
    "url": "git://github.com/d-low/jumpcloud-ui-assignment.git"
  },
  "private": false,
  "author": "JumpCloud and Mike DiLorenzo",
  "scripts": {
    "build-js": "browserify --debug public/js/app.js -o public/build/app.js",
    "watch-js": "nodemon --no-notify-update -e js -w public/js -x 'npm run build-js'",
    "build-sass": "node-sass public/sass/main.scss public/build/main.css",
    "watch-sass": "nodemon --no-notify-update -e scss -w public/sass -x 'npm run build-sass'",
    "build": "npm run build-js & npm run build-sass & node server.js",
    "watch": "npm run watch-js & npm run watch-sass & node server.js",
    "start": "npm run build"
  },
  "dependencies": {
    "babelify": "^5.0.4",
    "backbone": "^1.1.2",
    "backbone-route-control": "^0.1.0",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "envify": "^3.4.0",
    "express": "^4.14.0",
    "jquery": "^2.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react.backbone": "^0.9.0",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "browserify": "^9.0.7",
    "node-sass": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  }
}
